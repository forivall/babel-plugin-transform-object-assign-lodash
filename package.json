{
  "name": "babel-plugin-transform-object-assign-lodash",
  "version": "1.0.0",
  "description": "Replace Object.assign with a lodash/assign",
  "repository": "https://github.com/forivall/babel-plugin-transform-object-assign-lodash",
  "author": "Emily Marigold Klassen <forivall@gmail.com>",
  "contributors": [
    "Jed Watson"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "babel-plugin"
  ],
  "scripts": {
    "build": "babel -d lib src",
    "pretest": "npm run build",
    "test": "ava",
    "lint": "xo",
    "posttest": "npm run lint",
    "postinstall": "npm run build"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.22.0",
    "babel-core": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint-config-mxo": "^0.4.0",
    "xo": "^0.18.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "ignore": [
      "*.test.js"
    ]
  },
  "xo": {
    "extends": "mxo/esnext",
    "overrides": [
      {
        "files": [
          "**/*.test.js",
          "test/!(helpers|fixtures)/**/*.js"
        ],
        "extends": "mxo/ava"
      }
    ]
  }
}
